{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "ES5",
    "jsx": "react",
    "lib": [
      "esnext",
      "dom"
    ],
    "allowJs": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}


























